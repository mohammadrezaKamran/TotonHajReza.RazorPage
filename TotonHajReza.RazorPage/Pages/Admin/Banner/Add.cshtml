@page
@model TotonHajReza.RazorPage.Pages.Admin.Banner.AddModel
@{
    ViewData["title"] = "افزودن بنر";
    ViewData["selected"] = "order";
}
<form method="post"enctype="multipart/form-data">
    <div class="row">
        <div class="col-md-6">
            @Html.EditorFor(f => f.Link)
        </div>
        <div class="col-md-6">
            @Html.EditorFor(f => f.Position)
        </div>
        <div class="col-md-12">
            @Html.EditorFor(f => f.ImageFile)
        </div>
    </div>
    <button type="submit" class="btn btn-primary">
        ذخیره تغییرات
    </button>
</form>
