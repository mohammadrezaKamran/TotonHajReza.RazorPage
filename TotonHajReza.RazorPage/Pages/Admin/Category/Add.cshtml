@page
@model TotonHajReza.RazorPage.Pages.Admin.Category.AddModel
@{
    ViewData["title"] = "افزودن دسته بندی";
    ViewData["selected"] = "category";
}
<h3>@ViewData["title"]</h3>

<form class="card" method="post">
    <div class="card-body">
        <div class="row">
            <div class="col-md-6">
                @Html.EditorFor(f => f.Title)
            </div>
            <div class="col-md-6">
                @Html.EditorFor(f => f.Slug)
            </div>
            @Html.EditorFor(f => f.SeoData)
        </div>
    </div>
    <div class="card-footer text-right">
        <cancel>Cancel</cancel>  
        <button class="btn btn-success" type="submit">ثبت اطلاعات</button>
       
    </div>
</form>